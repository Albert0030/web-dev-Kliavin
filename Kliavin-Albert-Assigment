<script>
function getCookie(name) {
  let matches = document.cookie.match(new RegExp(
    "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
  ));
  return matches ? decodeURIComponent(matches[1]) : undefined;
}

var utm_source = getCookie('_utm_source');
var utm_medium = getCookie('_utm_medium');
var utm_campaign = getCookie('_utm_campaign');
var utm_term = getCookie('_utm_term');
var utm_content = getCookie('_utm_content');

$('input[name="utm_source"]').val(utm_source);
$('input[name="utm_medium"]').val(utm_medium);
$('input[name="utm_campaign"]').val(utm_campaign);
$('input[name="utm_term"]').val(utm_term);
$('input[name="utm_content"]').val(utm_content);
$('input[name="linkpage"]').val(window.location.href);
</script>
