    <script>
        document.addEventListener('DOMContentLoaded', () => {
            /** init gtm after 3500 seconds - this could be adjusted */
            setTimeout(initGTM, 3500);
            setTimeout(initGoogleAnalytics, 3500);
            setTimeout(initGoogleConversion, 3500);
            setTimeout(initGoogleTag, 3500);
            setTimeout(initYandexMetrika, 3500);
            setTimeout(initCalltracking, 3500);
            setTimeout(initJivosite, 3500);
        });

        document.addEventListener('scroll', initGTMOnEvent);
        document.addEventListener('mousemove', initGTMOnEvent);
        document.addEventListener('touchstart', initGTMOnEvent);

        function initGTMOnEvent(event) {
            initGTM();
            initGoogleAnalytics();
            initGoogleConversion();
            initGoogleTag();
            initYandexMetrika();
            initCalltracking();
            initJivosite();
            event.currentTarget.removeEventListener(event.type, initGTMOnEvent); // remove the event listener that got triggered
        }

        function initGTM() {
            if (window.gtmDidInit) {
                return false;
            }
            window.gtmDidInit = true; // flag to ensure script does not get added to DOM more than once.

            const script = document.createElement('script');
            script.type = 'text/javascript';
            script.async = true;
            script.onload = () => { dataLayer.push({ event: 'gtm.js', 'gtm.start': (new Date()).getTime(), 'gtm.uniqueEventId': 0 }); } // this part ensures PageViews is always tracked
            script.src = 'https://www.googletagmanager.com/gtm.js?id=GTM-TW6Q3FN';

            document.head.appendChild(script);
        }

        function initGoogleAnalytics() {
            if (window.googleAnalyticsDidInit) {
                return false;
            }
            window.googleAnalyticsDidInit = true; // flag to ensure script does not get added to DOM more than once.

            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-15005076-1']);
            _gaq.push(['_trackPageview']);

            (function() {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
        }

        <!-- Код тега ремаркетинга Google -->
        function initGoogleConversion() {
            if (window.googleConversionDidInit) {
                return false;
            }
            window.googleConversionDidInit = true; // flag to ensure script does not get added to DOM more than once.

            var google_conversion_id = 874065975;
            var google_custom_params = window.google_tag_params;
            var google_remarketing_only = true;

            const script_conv = document.createElement('script');
            script_conv.type = 'text/javascript';
            script_conv.src = '//www.googleadservices.com/pagead/conversion.js';

            document.head.appendChild(script_conv);
        }

        <!-- Global site tag (gtag.js) - Google Analytics -->
        function initGoogleTag() {
            if (window.googleTagDidInit) {
                return false;
            }
            window.googleTagDidInit = true; // flag to ensure script does not get added to DOM more than once.

            const script_tag = document.createElement('script');
            script_tag.type = 'text/javascript';
            script_tag.async = true;
            script_tag.src = 'https://www.googletagmanager.com/gtag/js?id=UA-118254687-1';

            document.head.appendChild(script_tag);

            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-118254687-1');
        }

        <!-- Yandex.Metrika counter -->
        function initYandexMetrika() {
            if (window.yandexMetrikaDidInit) {
                return false;
            }
            window.yandexMetrikaDidInit = true; // flag to ensure script does not get added to DOM more than once.

            (function (d, w, c) {
                (w[c] = w[c] || []).push(function() {
                    try {
                        w.yaCounter815550 = new Ya.Metrika({id:815550,
                            webvisor:true,
                            clickmap:true,
                            trackLinks:true,
                            accurateTrackBounce:true});
                    } catch(e) { }
                });

                var n = d.getElementsByTagName("script")[0],
                    s = d.createElement("script"),
                    f = function () { n.parentNode.insertBefore(s, n); };
                s.type = "text/javascript";
                s.async = true;
                s.src = (d.location.protocol == "https:" ? "https:" : "http:") + "//mc.yandex.ru/metrika/watch.js";

                if (w.opera == "[object Opera]") {
                    d.addEventListener("DOMContentLoaded", f, false);
                } else { f(); }
            })(document, window, "yandex_metrika_callbacks");
        }

        <!-- /call-traking -->
        function initCalltracking() {
            if (window.calltrackingDidInit) {
                return false;
            }
            window.calltrackingDidInit = true; // flag to ensure script does not get added to DOM more than once.

            const script_calltracking = document.createElement('script');
            script_calltracking.type = 'text/javascript';
            script_calltracking.async = true;
            script_calltracking.src = '//app.call-tracking.by/scripts/calltracking.js?e1a36bee-7024-4851-94a1-36dc2e34b261';

            document.head.appendChild(script_calltracking);
        }

        <!-- JIVOSITE CODE {literal} -->
        function initJivosite() {
           if (window.jivositeDidInit) {
                return false;
            }
            window.jivositeDidInit = true; // flag to ensure script does not get added to DOM more than once.

           (function(){ var widget_id = 'ax7oPqFBFv'; var s = document.createElement('script'); s.type = 'text/javascript'; s.async = true; s.src = '//code.jivosite.com/script/widget/'+widget_id; var ss = document.getElementsByTagName('script')[0]; ss.parentNode.insertBefore(s, ss);})();
        }
    </script>

